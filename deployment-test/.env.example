# Hyperliquid Wallet Address (Public Key)
HL_WL=0xYourPublicWalletAddressHere

# Hyperliquid Private Key (Keep this secret!)
# This can be your main wallet private key or an agent wallet private key.
HL_PK=0xYourPrivateKeyHere

# Agent Wallet Address (Optional)
# If you are using an Agent Wallet (recommended for security), put its address here.
# If this is set, the server will verify that HL_PK corresponds to this agent.
# AG_WL=0xYourAgentWalletAddressHere

# Enable direct interaction with Master Wallet (Optional)
# Set to "true" if you are using your Master Wallet's private key directly (HL_PK)
# and NOT an Agent Wallet. Default is "false" for safety to prevent accidental main wallet usage.
ENABLE_MASTER_INTERACTION=false
